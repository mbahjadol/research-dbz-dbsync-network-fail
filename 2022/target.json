{
    "name": "XXXXX",
    "config": {
        "connector.class": "io.debezium.connector.jdbc.JdbcSinkConnector",
        "tasks.max": "5",
        "connection.url": "XXXXX",
        "connection.username": "XXXXX",
        "connection.password": "XXXXX",

        "database.encrypt": "false" ,
        "auto.create": "true",
        "insert.mode": "upsert",
        "delete.enabled": "true",
        "primary.key.mode": "record_key",
        "schema.evolution": "basic",

        "topics": "XXXXX",
        "topic.prefix": "XXXXX",

        "transforms": "removePrefix",
        "transforms.removePrefix.type": "org.apache.kafka.connect.transforms.RegexRouter",
        "transforms.removePrefix.regex": "XXXXX_(.*)",  
        "transforms.removePrefix.replacement": "$1",
        
        "batch.size": "200",
        "retry.backoff.ms": "1000",

        "connection.pool.max.size": "10",
        "connection.attempts": "3",
        "connection.backoff.ms": "1000",

        "offset.flush.interval.ms": "2000",
        "poll.interval.ms": "500",
        "auto.create.tables": "true",

        "errors.log.enable": "true",
        "errors.log.include.messages": "true",       

        "errors.tolerance": "all",
        "errors.retry.timeout": "0",  
        "errors.retry.delay.max.ms": "10000",
        "errors.deadletterqueue.topic.name": "dlq.XXXXX",
        "errors.deadletterqueue.context.headers.enable": "true"
    }
}
